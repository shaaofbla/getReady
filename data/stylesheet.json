[
    {
        "selector": "node",
        "style": {
            "content": "data(id)",
            "text-halign": "center",
            "text-valign": "center",
            "text-wrap": "wrap",
            "text-max-width": "40px",
            "width": "label",
            "height": "label",
            "background-color": "blue",
            "shape": "ellipse",
            "padding": "6px"
        }
    },


    {
        "selector": "[id *= 'ZuV']",
        "style": {
            "content": "data(label)",
            "background-color":"#6afbd4",
            "border-color":"#05C793",
            "border-width":"1px",
            "shape": "rectangle",
            "font-size": "4"
        }
    },
    {
        "selector": "[id *= 'FuR']",
        "style": {
            "content": "data(label)",
            "border-color": "#EF4365",
            "background-color": "#f7a1b2",
            "border-width": "1px",
            "shape": "rectangle",
            "font-size": "4"
        }
    },
    {
        "selector": "[id *= 'GFDuZ']",
        "style": {
            "content": "data(label)",
            "background-color":"#ffe6ad",
            "border-color": "#FFCE5C",
            "border-width": "1px",
            "shape": "rectangle",
            "font-size": "4"
        }
    },
    {
        "selector": "[level < '3']",
        "style": {
            "shape": "ellipse",
            "content": "_",
            "font-size": "1"
        }
    },
    {
        "selector": "[level < '2']",
        "style": {
            "shape": "round-rectangle",
            "content": "data(label)",
            "font-size": "12"

        }
    },
    {
        "selector": "edge",
        "style" : {
            "curve-style": "straight-triangle"
        }
    },
    {
        "selector": "[classes *= 'A']",
        "style" : {
            "background-color": "#3CAEA3"
        }
    },
    {
        "selector": "[classes *= 'both']",
        "style" : {
            "background-color": "#F6D55C"
        }
    },
    {
        "selector": "[classes *= 'B']",
        "style" : {
            "background-color": "#ED553B"
        }
    }

]